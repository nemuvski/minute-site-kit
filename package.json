{
  "name": "minute-site-kit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist node_modules/.vite",
    "typecheck": "tsc --noEmit",
    "lint:es": "eslint --ext .js,.ts .",
    "lint:style": "stylelint './**/*.{scss,css}'",
    "lint:style:fix": "stylelint --fix './**/*.{scss,css}'",
    "format": "prettier --check './**/*.{js,ts,scss,css,html}'",
    "format:fix": "prettier --write './**/*.{js,ts,scss,css,html}'",
    "dev": "vite",
    "build": "npm run clean && tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.7.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "stylelint": "^14.9.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-sass-glob-import": "^1.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
